<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.spearre91.adviseme" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Main"
            android:label="AdviseMe" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ResultsPage"
            android:label="@string/title_activity_results_page"
            android:parentActivityName = "com.example.spearre91.adviseme.Main">
        <meta-data
            android:name = "android.support.PARENT_ACTIVITY"
            android:value = "com.example.spearre91.adviseme.Main" />
        </activity>
    </application>

</manifest>
